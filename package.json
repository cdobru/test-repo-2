    "consolidate": "0.14.5",
    "dustjs-helpers": "1.5.0",
    "dustjs-linkedin": "3.0.0",
    "ejs": "2.5.3",
    "ejs-locals": "1.0.2",
    "errorhandler": "1.2.0",
    "express": "4.12.4",
    "express-fileupload": "0.0.5",
    "express-session": "^1.17.2",
    "file-type": "^8.1.0",
    "hbs": "^4.0.4",
    "humanize-ms": "1.0.2",
    "jq": "^1.7.2",
    "jquery": "^2.2.4",
